{
    "name": "attendkal-backend",
    "version": "1.0.0",
    "description": "Attendkal Backend API - University attendance tracker",
    "main": "dist/server.js",
    "engines": {
        "node": ">=20 <21"
    },
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/server.js",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:e2e": "jest --testPathPattern=e2e",
        "prisma:validate": "prisma validate",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:deploy": "prisma migrate deploy",
        "prisma:push": "prisma db push",
        "prisma:studio": "prisma studio",
        "db:generate": "npm run prisma:generate",
        "db:migrate": "npm run prisma:migrate",
        "db:reset": "prisma migrate reset",
        "db:studio": "npm run prisma:studio"
    },
    "keywords": [
        "attendance",
        "university",
        "tracker",
        "api"
    ],
    "author": "Attendkal Team",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.10.0",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "pg": "^8.11.3",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.11.30",
        "@types/pg": "^8.10.2",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.46.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.0",
        "prisma": "^5.22.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.6"
    }
}