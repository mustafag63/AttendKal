{
    "name": "attendkal-backend",
    "version": "1.0.0",
    "description": "Attendkal Backend API - University attendance tracker",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:reset": "prisma migrate reset",
        "db:studio": "prisma studio"
    },
    "keywords": [
        "attendance",
        "university",
        "tracker",
        "api"
    ],
    "author": "Attendkal Team",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "dotenv": "^16.3.1",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "@prisma/client": "^5.2.0",
        "zod": "^3.21.4",
        "express-rate-limit": "^6.10.0",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/cors": "^2.8.13",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.9",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/pg": "^8.10.2",
        "typescript": "^5.1.6",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint": "^8.46.0",
        "prettier": "^3.0.0",
        "prisma": "^5.2.0",
        "tsconfig-paths": "^4.2.0"
    }
}